	sub Session_OnStart
	  application.Lock()
	  application("onlineUser")=application("onlineUser")+1
	  application.UnLock()
	end sub
	
	sub Session_OnEnd
	  application.Lock()
	  application("onlineUser")=application("onlineUser")-1
	  application.UnLock()
	end sub
